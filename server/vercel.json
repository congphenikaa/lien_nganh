{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/payment/callback",
            "dest": "/server.js",
            "methods": ["GET", "POST"]
        },
        {
            "src": "/(.*)",
            "dest": "/server.js",
            "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS", "HEAD"]
        }
    ]
}